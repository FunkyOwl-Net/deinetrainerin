{
    "name": "deinetrainerin/app",
    "description": "Relaunch von deineTrainerin.at als Laravel 11 Projekt.",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "laravel/framework": "^11.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.19",
        "phpunit/phpunit": "^10.5",
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "test": "@php vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
